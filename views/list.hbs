<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>
  </head>
  <body>
    <h1>List of {{type}}</h1>
    <div id="list-container">
      <ul id="list">
        {{#each list}}
          <li class="list-item">
            <a href={{this.url}}>{{this.name}}</a>
          </li>
        {{/each}}
      </ul>
    </div>
  </body>
</html>